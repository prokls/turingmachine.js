    "blank" : " ",
    "cursor" : 0,
    "data" : ["0", "0"]
  },
  "program": [
    ["0", "Start", "0", "RIGHT", "Even"],
    ["0", "Even", "0", "RIGHT", "Even"],
    ["0", "Odd", "0", "RIGHT", "Odd"],
    ["1", "Start", "1", "RIGHT", "Odd"],
    ["1", "Even", "1", "RIGHT", "Odd"],
    ["1", "Odd", "1", "RIGHT", "Even"],
    [" ", "Even", "0", "STOP", "End"],
    [" ", "Odd", "1", "STOP", "End"]
  ],
  "state" : "Start",
  "final_states" : ["End"],
